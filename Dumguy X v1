    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Click%20Teleport.txt"))()
end)

MainSection:NewButton("Aimbot + ESP", "Aims for you and shows everyone!", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Universal%20ESP%20+%20Aimbot.txt"))()
end)

MainSection:NewButton("High Hips", "Walk like normal, but a little taller.", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/High%20Hips.txt"))()
end)

MainSection:NewButton("Remove Legs", "Removes the legs from your player's character.", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Remove%20Legs.txt"))()
end)

MainSection:NewButton("Teleport To Player", "Teleports your player's character to the selected player's character", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Teleport%20To%20Player.txt"))()
end)

MainSection:NewButton("Float Character", "Makes your player's character float in the air.", function()
